        <TextBox Height="371" HorizontalAlignment="Left" Margin="12,261,0,0" Name="textBox1" VerticalAlignment="Top" Width="407" TextWrapping="Wrap" />
        <TextBox Height="371" HorizontalAlignment="Left" Margin="466,261,0,0" Name="textBox2" TextWrapping="Wrap" VerticalAlignment="Top" Width="407" />
        <Button Content="Save to File" Height="23" HorizontalAlignment="Left" Margin="529,0,0,0" Name="button3" VerticalAlignment="Top" Width="122" Click="button3_Click" />
